class Solution {
public:
    bool isPalindrome(int x) {
        //рд╢реБрдн рджрд┐рдкрд╛рд╡рд▓реАЁЯкФЁЯОЖ
        if(x<0)
            return false;
        
        long long temp = x;
        long long rev = 0;
        
        while (temp != 0)
        {
            int digit = temp % 10;
            rev = rev*10 + digit;
            temp = temp/ 10;
        }
        
        return (rev == x) ;
    }
};
